{
  "extends": "./paths.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "react-jsx",
		"types": ["vite-plugin-svgr/client", "node"],
		"typeRoots": ["node_modules/@types"]
  },
	"exclude": ["node_modules"]
}
