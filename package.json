{
  "name": "simple-file-manager",
  "version": "0.0.1",
  "description": "Simple file manager created using React, TypeScript and Electron",
  "main": "./electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mikutut/simple-file-manager"
  },
  "author": "Marcin \"Mikut\" Miku≈Ça",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev:build": "webpack --config webpack.dev.config.js",
    "dev:start": "yarn dev:build && ELECTRON_IS_DEV=1 electron ./build/electron.js",
    "dev:serve": "webpack serve --config webpack.devServer.config.js",
    "dev:runelectron": "ELECTRON_IS_DEV=1 electron ./build/electron.js",
    "dev:serve:start": "concurrently -k \"yarn dev:serve\" \"wait-on tcp:3000 && cross-env IS_DEV_SERVER=1 yarn dev:runelectron\"",
    "prod:build": "webpack --config webpack.prod.config.js",
    "prod:start": "yarn prod:build && electron ./prod/build/electron.js"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-reloader": "^1.2.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.4",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.7.2"
  }
}
